<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demokotlin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.RecyclerViewList"></activity>
        <activity android:name="revo.adnan.com.learnkotln.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="revo.adnan.com.learnkotln.SimpleListView" />
        <activity android:name="revo.adnan.com.learnkotln.CustomListView" />
        <activity android:name="revo.adnan.com.learnkotln.TabLayoutActivity" />
    </application>

</manifest>